.index-floor-data .floor-title {
  margin-top: 20rpx;
}
.index-floor-data .floor-title image {
  width: 100%;
}
.index-floor-data .floor-content {
  overflow: hidden;
}
.index-floor-data .floor-content navigator {
  float: left;
  width: 33.33%;
}
.index-floor-data .floor-content navigator:nth-child(n+2) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index-floor-data .floor-content navigator image {
  width: 100%;
  height: 100%;
}
.index-floor-data .floor-content navigator:nth-child(2),
.index-floor-data .floor-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
